{
  "server_info": {
    "name": "vps-01",
    "region": "Singapore"
  },
  "domains": {
    "wildcard_base": "proxy.example.com",
    "cdn_domain": "cdn.example.com",
    "subscription": "sub.example.com"
  },
  "protocols": {
    "xhttp": {
      "enabled": true
    },
    "grpc": {
      "enabled": true
    }
  },
  "haproxy": {
    "enabled": true,
    "frontend_port": 443,
    "stats_user": "admin",
    "stats_password": "auto-generate"
  },
  "certificates": {
    "auto_issue": true,
    "email": "admin@example.com",
    "provider": "letsencrypt",
    "wildcard": true,
    "dns_provider": "cloudflare",
    "dns_api_token": null
  },
  "warp_outbound": {
    "enabled": false,
    "socks_address": "127.0.0.1",
    "socks_port": 25344,
    "routing_mode": "selective",
    "route_domains": [
      "geosite:netflix",
      "geosite:disney",
      "geosite:hbo",
      "geosite:spotify"
    ],
    "block_bt": true
  },
  "proton_outbound": {
    "enabled": false,
    "socks_address": "127.0.0.1",
    "socks_port": 25345
  },
  "subscription": {
    "enabled": true,
    "login_user": "admin",
    "login_password": "auto-generate"
  },
  "code_server": {
    "enabled": false,
    "password": "auto-generate"
  }
}